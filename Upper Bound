int upperBound(vector<int> &arr, int x, int n)
{
	// Write your code here.
    int ans=arr.size();
    int low=0, high=ans-1;
    while(low<=high)
    {
        int mid=(low+high)/2;
        if(arr[mid]>x)
        {
            ans=mid;
            high=mid-1;
        }
        else
        {
            low=mid+1;
        }
    }
    return ans;
}
